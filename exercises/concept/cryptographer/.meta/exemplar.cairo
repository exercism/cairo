pub fn add_messages(message1: felt252, message2: felt252) -> felt252 {
    message1 + message2
}

pub fn subtract_messages(message1: felt252, message2: felt252) -> felt252 {
    message1 - message2
}

pub fn multiply_messages(message1: felt252, message2: felt252) -> felt252 {
    message1 * message2
}
